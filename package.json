{
  "name": "autoinjection",
  "version": "0.0.8",
  "description": "Automatic Dependency Injectoin for TypeScript.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "ttsc",
    "test": "TS_NODE_COMPILER=ttypescript TS_NODE_PROJECT=test/tsconfig.json ava"
  },
  "keywords": [
    "TypeScript",
    "DI",
    "Dependency Injection"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/No-YE/autoinjection.git"
  },
  "author": "No-YE",
  "license": "MIT",
  "peerDependencies": {
    "typescript": ">=4.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "ava": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*.ts"
    ]
  }
}
